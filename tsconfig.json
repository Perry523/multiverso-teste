{
    "compilerOptions": {
      "sourceMap": true,
      "noImplicitAny": false,
      "module": "commonjs",
      "target": "es5",
      "lib": [
        "esnext",
        "dom",
        "dom.iterable"
      ],
      "removeComments": true,
      "allowSyntheticDefaultImports": true,
      "jsx": "react-jsx",
      "allowJs": true,
      "baseUrl": "./",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "downlevelIteration": true,
      "paths": {
        "components/*": [
          "src/components/*"
        ]
      }
    },
    "include": [
      "resources/js/**/*",
      // "resources/js/pages/*",
      "resources/js/components/**/*",
    ]
}